                            <input name="sudent-photo" type="file"  id="uploade_student_photo">
                        </div>
                        <div class="my-3">
                             <input type="submit" class="btn btn-sm btn-primary w-100" value="create" name="" id="">
                        </div>
    

                    </form>
                    
                  
                </div>
            </div>
        </div>
    </div>
</div>

