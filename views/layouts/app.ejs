<script type="text/javascript" src="https://cdn.datatables.net/v/bs5/dt-1.12.1/datatables.min.js"></script>

  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>

<script>

  // make datatable
$('.soro-data-table').DataTable();


// student photo priview
const studentImage = document.querySelector('#uploade_student_photo');
const studentImageTag = document.querySelector('.preload-student');


if (studentImage) {
    studentImage.onchange = (e) => {
    

    const imageLink = URL.createObjectURL(e.target.files[0]);

    studentImageTag.setAttribute('src', imageLink)
    
  }
}

  // deletebtn massage
  const delete_btn = document.querySelectorAll('.delet-btn')

  if (delete_btn) {
          delete_btn.forEach(item => {
            item.onclick = (e) => {
              const conf = confirm('Are you sure.?')

              if (conf) { 
                return true;
              } else {
                return false;
                
              }

            }
      })

  }






</script>
     
</body>

</html>